$colorBtnTextSecondary: {{ settings.color_btn_text_secondary }};

// Opacity
$header_bg_opacity: {{ settings.header_bg_opacity }};

// Box Shadow
$boxShadow: 2px 2px 12px rgba(0, 0, 0, 0.25);

// Typography
$fontLight: 300;
$fontRegular: 400;
$fontBold: 700;
$lineHeight: 1.75;

// Space & Borders
$gutter: 10px;
$borderRadius: 2px;
